{
  "mcpServers": {
    "docs": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--api-key", "REPLACE_WITH_CONTEXT7_KEY"],
      "start_on_launch": true
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "C:\\dev"],
      "start_on_launch": false
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "start_on_launch": false
    }
  }
}

